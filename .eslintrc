{
  "globals": {
    "qp": true,
    "app": true,
    "debug": true,
    "log": true
  },
  "rules": {
    "indent": [ 2, 2 ],
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "always" ],
    "no-unused-vars": 0,
    "no-undef": 0
  },
  "env": {
    "es6": false,
    "browser": true,
    "node": false
  },
  "extends": "eslint:recommended"
}
